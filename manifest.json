{
  "manifest_version": 2,

  "name": "SACE",
  "description": "Super Awesome Chrome Extension. It allows you to redirect urls and stuff",
  "version": "0.1",

  "permissions": [
    "tabs",
    "webRequest", "webRequestBlocking",
    "browsingData",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "SACE currently active"
  },

  "devtools_page": "devtools/devtools.html"
}